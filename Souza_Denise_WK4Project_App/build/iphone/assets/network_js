exports.netCheck=function(e){console.log("HTTP");var r=Ti.Network.createHTTPClient({onload:function(){var e=[],r=JSON.parse(this.responseText);for(i=0;i<r.length;i++){var a=r[i].streamable;if(1==a){var t={title:r[i].title,user:r[i].user.username,stream:r[i].stream_url+"?&client_id=cc7f901025ad59c9e06ea0f140fd6e58",genre:r[i].genre,artwork:r[i].artwork_url,duration:r[i].duration};if(t.artwork){var o=r[i].artwork_url.replace("large","original");t.artwork=o}else t.artwork="images/soundcloud_placeholder.jpg";if(t.title){var s="POSTED BY: "+r[i].user.username.toUpperCase();console.log(s),t.user=s}e.push(t)}}uiLoad.playlistBuild(e)},onerror:function(){new dataLoad;Ti.Network.online||alert("Could not find internet connection!")}});r.open("GET",e),r.send()};