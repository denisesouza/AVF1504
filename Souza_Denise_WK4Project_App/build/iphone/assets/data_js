exports.orm=function(){var e=Titanium.Database.open("database");e.execute("CREATE TABLE IF NOT EXISTS favorites (id INTEGER PRIMARY KEY, title TEXT, user TEXT, stream TEXT, genre TEXT, artwork TEXT, duration INTEGER)"),e.close(),this.read=function(){for(var e=Titanium.Database.open("database"),a=e.execute("SELECT id, title, user, stream, genre, artwork, duration FROM favorites"),i=[],t={};a.isValidRow();)i.push(t={title:a.fieldByName("title"),user:a.fieldByName("user"),stream:a.fieldByName("stream"),genre:a.fieldByName("genre"),artwork:a.fieldByName("artwork"),duration:a.fieldByName("duration"),id:a.fieldByName("id")}),a.next();0==i.length?alert("Please add songs to playlist!"):uiLoad.playlistBuild(i),a.close(),e.close()},this.create=function(e){var a=Titanium.Database.open("database");a.execute("INSERT INTO favorites (title, user, stream, genre, artwork, duration) VALUES (?, ?, ?, ?, ?, ?)",e.title,e.user,e.stream,e.genre,e.artwork,e.duration),a.close()},this.dele=function(){var e=Titanium.Database.open("database");e.execute("DELETE FROM favorites"),e.close()}};