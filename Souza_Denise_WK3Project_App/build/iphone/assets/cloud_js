var Cloud=require("ti.cloud");Cloud.debug=!0,exports.appCloudLogin=function(){Ti.Network.online&&Cloud.Users.login({login:"com.souzadenise.wk3app",password:"12345"},function(e){e.success?(uiLoad.entryBuild(),Ti.API.info("Logged in user, id = "+e.users[0].id+", session ID = "+Cloud.sessionId)):Ti.API.info("Login failed.")})},exports.cloudSave=function(e){Cloud.Objects.create({classname:"playlist",fields:{title:e.title,stream:e.stream,genre:e.genre,duration:e.duration,artwork:e.artwork}},function(e){if(e.success){var r=e.playlist[0];alert("Success:\nid: "+r.id+"\n: "+r.title+"\nStream URL: "+r.stream+"\nGenre: "+r.genre+"\nDuration:"+r.duration+"\nAlbum Art:"+r.artwork+"\ncreated_at:"+r.created_at)}else alert("Error:\n"+(e.error&&e.message||JSON.stringify(e)))})};