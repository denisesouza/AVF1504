loadWeather=require("weatherData").db,exports.check=function(e){var t=Ti.Network.createHTTPClient({onload:function(){var e=new loadWeather;e.delete();var t=JSON.parse(this.responseText);console.log(t);var o={ftemp:t.current_observation.feelslike_f,ctemp:t.current_observation.feelslike_c,city:t.current_observation.display_location.city,state:t.current_observation.display_location.state_name,wind:t.current_observation.wind_mph,low:t.forecast.simpleforecast.forecastday[0].low.fahrenheit,high:t.forecast.simpleforecast.forecastday[0].high.fahrenheit};e.create(o)},onerror:function(){var e=new loadWeather;if(!Ti.Network.online){var t={ftemp:"--",ctemp:"--",city:"--",state:"--",wind:"--",low:"--",high:"--"};e.create(t),alert("Could not connect to network!")}}});t.open("GET",e),t.send()};