loadWeather=require("weatherData").db;var cloudSave=require("cloud");exports.check=function(e){var o=Ti.Network.createHTTPClient({onload:function(){var e=new loadWeather;e.delete();var o=JSON.parse(this.responseText);console.log(o);var t={ftemp:o.current_observation.feelslike_f,ctemp:o.current_observation.feelslike_c,city:o.current_observation.display_location.city,state:o.current_observation.display_location.state_name,wind:o.current_observation.wind_mph,low:o.forecast.simpleforecast.forecastday[0].low.fahrenheit,high:o.forecast.simpleforecast.forecastday[0].high.fahrenheit,lowC:o.forecast.simpleforecast.forecastday[0].low.celsius,maxWind:o.forecast.simpleforecast.forecastday[0].maxwind.mph,phase:o.moon_phase.phaseofMoon,hemisphere:o.moon_phase.hemisphere};console.log(t.phase,t.hemisphere),e.create(t),cloudSave.cloudSave(t)},onerror:function(){var e=new loadWeather;if(!Ti.Network.online){var o={ftemp:"--",ctemp:"--",city:"--",state:"--",wind:"--",low:"--",high:"--"};e.create(o),alert("Could not connect to network!")}}});o.open("GET",e),o.send()};