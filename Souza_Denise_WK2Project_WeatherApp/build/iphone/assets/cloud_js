exports.cloudSave=function(e){Cloud.Objects.create({classname:"weather",fields:{ftemp:e.ftemp,ctemp:e.ctemp,city:e.city,state:e.state,wind:e.wind}},function(e){if(e.success){var i=e.weather[0];alert("Success:\nid: "+i.id+"\nFahrenheit: "+i.ftemp+"\nCelsius: "+i.ctemp+"\nCity: "+i.city+"\nState:"+i.state+"\nWind:"+i.wind+"mph\ncreated_at:"+i.created_at)}else alert("Error:\n"+(e.error&&e.message||JSON.stringify(e)))})};