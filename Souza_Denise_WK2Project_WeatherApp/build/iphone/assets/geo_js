var network=require("network").check;exports.geo=function(){Ti.Geolocation.getCurrentPosition(function(e){var o=Ti.Platform.osname;if("android"===o){var t="-71.0822085",i="41.9604969",n="http://api.wunderground.com/weather/api/8ad159642a519b70/conditions/astronomy/forecast/bestfct/q/"+i+","+t+".json";network(n)}else if(Ti.Geolocation.AUTHORIZATION_AUTHORIZED){var t=e.coords.longitude,i=e.coords.latitude,n="http://api.wunderground.com/weather/api/8ad159642a519b70/conditions/astronomy/forecast/bestfct/q/"+i+","+t+".json";network(n)}else alert("Cannot get your current location, to retrieve updated weather data!")})};