var view=require("ui").windowView;exports.db=function(){var e=Titanium.Database.open("database");e.execute("CREATE TABLE IF NOT EXISTS weatherData (id INTEGER PRIMARY KEY, ftemp TEXT, ctemp TEXT, city TEXT, state TEXT, wind TEXT, low TEXT, high TEXT, lowC TEXT, maxWind TEXT, phase TEXT, hemisphere TEXT)"),e.close(),this.read=function(){var e=Ti.Database.open("database"),t=e.execute("SELECT * FROM weatherData"),o={ftemp:t.fieldByName("ftemp"),ctemp:t.fieldByName("ctemp"),city:t.fieldByName("city"),state:t.fieldByName("state"),wind:t.fieldByName("wind"),low:t.fieldByName("low"),high:t.fieldByName("high"),lowC:t.fieldByName("lowC"),maxWind:t.fieldByName("maxWind"),phase:t.fieldByName("phase"),hemisphere:t.fieldByName("hemisphere")};t.close(),e.close(),view(o)},this.create=function(e){console.log(e);var t=Ti.Database.open("database");t.execute("INSERT INTO weatherData (ftemp, ctemp, city, state, wind, low, high, lowC, maxWind, phase, hemisphere) VALUES (?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?)",e.ftemp,e.ctemp,e.city,e.state,e.wind,e.low,e.high,e.lowC,e.MaxWind,e.phase,e.hemisphere),t.close(),this.read()},this.delete=function(){var e=Ti.Database.open("database");e.execute("DELETE FROM weatherData"),e.close()}};