exports.windowView=function(e){console.log(e);var t=Ti.UI.createWindow({backgroundImage:"background.jpg",fullscreen:!0}),o=Ti.UI.createWindow({backgroundImage:"backgroundNight.jpg",fullscreen:!0}),r=Ti.UI.createWindow({backgroundImage:"backgroundForecast.jpg",fullscreen:!0}),a=Ti.UI.createView({top:"2%",bottom:"50%",left:"2%",right:"2%",borderRadius:"8dp",borderColor:"#fff",backgroundColor:"#80000000"}),i=Ti.UI.createLabel({text:"Current Weather",color:"#fff",top:"1%",font:{fontSize:"24dp"}}),n=Ti.UI.createLabel({text:e.ftemp+"°",bottom:"50%",left:"10%",color:"#fff",font:{fontSize:"96dp"}}),f=Ti.UI.createLabel({text:e.ctemp+"°c",bottom:"15%",left:"5%",color:"#fff",font:{fontSize:"40dp"}}),d=Ti.UI.createLabel({text:e.city,top:"7%",left:"5%",color:"#fff",font:{fontSize:"40dp"}}),c=Ti.UI.createLabel({text:e.state,color:"#fff",top:"18%",left:"5%",font:{fontSize:"24dp"}}),l=Ti.UI.createLabel({text:"Wind: "+e.wind+" mph",color:"#fff",right:"5%",top:"7%",font:{fontSize:"20dp"}}),s=Ti.UI.createLabel({text:"L : "+e.low+"°",color:"#fff",left:"5%",bottom:"2%",font:{fontSize:"22dp"}}),p=Ti.UI.createLabel({text:"H : "+e.high+"°",color:"#fff",left:"20%",bottom:"2%",font:{fontSize:"22dp"}}),u=Ti.UI.createView({top:"2%",bottom:"50%",left:"2%",right:"2%",borderRadius:"10dp",borderColor:"#fff",backgroundColor:"#80000000"}),h=Ti.UI.createLabel({text:"Projected Hi & Low Temp",color:"#fff",top:"1%",font:{fontSize:"24dp"}}),b=Ti.UI.createLabel({text:"H : "+e.high+"º L : "+e.low+"º",color:"#fff",bottom:"40%",left:"35%",font:{fontSize:"44dp"}}),m=(Ti.UI.createLabel({text:"Max Wind: "+e.maxWind+" mph",color:"#fff",top:"7%",right:"5%",font:{fontSize:"20dp"}}),Ti.UI.createLabel({text:e.state,color:"#fff",top:"20%",left:"5%",font:{fontSize:"24dp"}})),g=Ti.UI.createLabel({text:e.city,color:"#fff",top:"7%",left:"5%",font:{fontSize:"40dp"}}),w=Ti.UI.createView({top:"1%",bottom:"58%",left:"2%",right:"2%",borderRadius:"10dp",borderColor:"#fff",backgroundColor:"#80000000"}),T=Ti.UI.createLabel({text:e.state,color:"#fff",top:"15%",left:"5%",font:{fontSize:"22dp"}}),I=Ti.UI.createLabel({text:e.city,color:"#fff",top:"7%",left:"5%",font:{fontSize:"28dp"}}),L=Ti.UI.createLabel({text:"H : "+e.high+"º L : "+e.low+"º",color:"#fff",bottom:"2%",left:"5%",font:{fontSize:"44dp"}}),S=Ti.UI.createLabel({text:e.low+"º",color:"#fff",bottom:"12%",left:"5%",font:{fontSize:"92dp"}}),x=(Ti.UI.createLabel({text:e.lowC+" ºc",color:"#fff",bottom:"2%",left:"5%",font:{fontSize:"28dp"}}),Ti.UI.createLabel({text:"Tonight's Weather",color:"#fff",top:"1%",font:{fontSize:"24dp"}})),U=Ti.UI.createLabel({text:"Moon Phase: "+e.phase+"/"+e.hemisphere,color:"#fff",top:"10%",right:"10%",font:{fontSize:"20dp"}});t.addEventListener("swipe",function(e){console.log(e.direction),"left"==e.direction?o.open():"right"===e.direction?alert("Can't Seem to get my 3rd window to open"):"up"===e.direction}),o.addEventListener("swipe",function(e){"down"==e.direction?r.open():"left"==e.direction&&r.open()}),r.addEventListener("swipe",function(e){"right"==e.direction&&o.close()}),w.add(S),w.add(x),w.add(I),w.add(T),w.add(U),w.add(L),a.add(n),a.add(f),a.add(d),a.add(c),a.add(l),a.add(n),a.add(i),a.add(s),a.add(p),u.add(b),u.add(g),u.add(m),u.add(h),r.add(u),o.add(w),t.add(a),t.open()};